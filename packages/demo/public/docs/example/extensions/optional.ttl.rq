PREFIX schema: <http://schema.org/>
CONSTRUCT {
  ?resource1 schema:givenName ?resource4.
  ?resource1 schema:familyName ?resource5.
}
WHERE {
  { ?resource1 schema:givenName ?resource4. }
  UNION
  { ?resource1 schema:familyName ?resource5. }
  OPTIONAL { ?resource1 (schema:address/schema:addressRegion) ?resource3. }
  OPTIONAL { ?resource1 (schema:address/schema:addressRegion) ?resource3. }
  FILTER((!(BOUND(?resource3))) || (?resource3 = "TX"))
}