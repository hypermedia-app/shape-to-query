PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX schema: <http://schema.org/>
CONSTRUCT {
  ?resource1 rdf:type schema:Person.
  ?resource1 ?resource3 ?resource4.
}
WHERE {
  ?resource1 rdf:type schema:Person;
    ?resource3 ?resource4.
  FILTER(?resource3 = schema:knows)
  ?resource4 schema:jobTitle ?resource5.
  FILTER(REGEX(?resource5, "physicist"))
}