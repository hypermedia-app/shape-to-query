PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX schema: <http://schema.org/>
CONSTRUCT {
  ?resource1 rdf:type schema:Periodical.
  ?resource1 <http://example.org/numberOfArticles> ?resource3.
}
WHERE {
  ?resource1 rdf:type schema:Periodical.
  {
    SELECT (COUNT(?resource4) AS ?resource3) WHERE {
      ?resource1 schema:publication ?resource4;
        rdf:type schema:Periodical.
    }
  }
}