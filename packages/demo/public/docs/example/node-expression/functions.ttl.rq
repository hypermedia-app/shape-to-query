PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX schema: <http://schema.org/>
CONSTRUCT {
  ?resource1 rdf:type schema:Person.
  ?resource1 <http://example.org/drivingExperienceYears> ?resource3.
}
WHERE {
  ?resource1 rdf:type schema:Person;
    (<http://example.org/drivingLicense>/<http://example.org/issueDate>) ?resource4.
  BIND((YEAR(NOW())) - (YEAR(?resource4)) AS ?resource3)
}