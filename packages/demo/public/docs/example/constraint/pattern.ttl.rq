PREFIX schema: <http://schema.org/>
CONSTRUCT {
  ?resource1 schema:givenName ?resource4.
  ?resource1 schema:familyName ?resource3.
}
WHERE {
  { ?resource1 schema:givenName ?resource4. }
  UNION
  { ?resource1 schema:familyName ?resource3. }
  ?resource1 schema:familyName ?resource3.
  FILTER(REGEX(?resource3, "^[bc]", "i"))
}