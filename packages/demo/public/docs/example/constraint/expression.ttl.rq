PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX schema: <http://schema.org/>
CONSTRUCT {
  ?resource1 rdf:type schema:Person.
  ?resource1 schema:jobTitle ?resource4.
}
WHERE {
  ?resource1 rdf:type schema:Person;
    schema:jobTitle ?resource4, ?resource3.
  FILTER(CONTAINS(?resource3, "physicist"))
}